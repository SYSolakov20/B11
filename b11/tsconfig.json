{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "ignoreDeprecations": "5.0",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }  ,
    "allowJs": true,
     "strictNullChecks":false
  },

  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
