generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  discordId String? @unique
  githubId  String? @unique

  email     String
  username  String
  firstName String?
  lastName  String?
}
